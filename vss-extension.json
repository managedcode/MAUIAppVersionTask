{
  "manifestVersion": 1,
  "id": "build-release-task",
  "name": "Fabrikam Build and Release Tools",
  "version": "0.0.3",
  "publisher": "Managed-code",
  "targets": [
      {
          "id": "Microsoft.VisualStudio.Services"
      }
  ],    
  "description": "Tools for building/releasing with Managed-code. Includes one build/release task.",
  "categories": [
      "Azure Pipelines"
  ],
  "icons": {
      "default": "images/extension-icon.png"
  },
  "files": [
      {
          "path": "tasks"
      }
  ],
  "contributions": [
      {
          "id": "custom-build-release-task",
          "type": "ms.vss-distributed-task.task",
          "targets": [
              "ms.vss-distributed-task.tasks"
          ],
          "properties": {
              "name": "tasks"
          }
      }
  ]
}